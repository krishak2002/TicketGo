<script>
     function go(id,st)
            {
                 //alert (st);
                 var st1;
                 if(st==='pending')
                 {
                    st1='approve';
                     document.getElementById("id").value='approve';
                 }
                 else
                 {
                     st1='pending';
                     document.getElementById("id").value='pending';
                 }
                 //alert (st1);
                 
                var url = "../adminallow?st1="+ st1 +"&id="+id;

                    fetch(url)
                            .then(response => response.text())
                            .then(ans => renderAsHtml(ans));
                
                }      
                
    
    function renderAsHtml(ans)
                    {   
                        if(ans==="true")
                        {   
                            alert("SUCCESS");
                            
                            
                        }
                        
                        else
                        {
                            alert("FAIL");
                        }

                        
